  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="member.mapper">
	<insert id="memberInsert" parameterType="web.vo.Member">
		insert into
		Member(id,pw,name) values( #{id}, #{pw}, #{name} )
	</insert>

</mapper>
  