<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

  <mapper namespace="member.mapper">
  
  	<select id="login" parameterType="web.vo.Member" resultType="String">
  		select name from Member where id=#{id} and pw=#{pw}
  	</select>
  	
  	<insert id="memberInsert"  parameterType="web.vo.Member" >
  		insert into Member(id,pw,name) values( #{id}, #{pw}, #{name} )
  	</insert>
  	
  </mapper>